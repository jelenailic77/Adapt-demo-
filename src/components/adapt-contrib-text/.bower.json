  "framework": ">=5.19.1",
  "homepage": "https://github.com/adaptlearning/adapt-contrib-text",
  "bugs": "https://github.com/adaptlearning/adapt-contrib-text/issues",
  "component": "text",
  "targetAttribute": "_text",
  "displayName": "Text",
